<Window x:Class="elQuizer_CS.Report"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Report" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="27"/>
        </Grid.RowDefinitions>
        <ScrollViewer ScrollViewer.CanContentScroll="True">
            <StackPanel Margin="0,0,0,0" Name="table_sp">
            
            </StackPanel>
        </ScrollViewer>

        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Button Margin="2">Save</Button>
            <Button Grid.Column="1" Margin="2">Close</Button>
        </Grid>
        <StackPanel Grid.Row="1" Orientation="Horizontal"> 
            
        </StackPanel>

    </Grid>
</Window>
